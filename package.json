{
  "name": "koala-app",
  "version": "1.0.0",
  "description": "The skeleton application for Koala-Ts application",
  "keywords": [
    "Koalats",
    "Framework"
  ],
  "license": "ISC",
  "author": "",
  "type": "module",
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=11.0.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "tsx watch src/index.ts | pino-pretty",
    "test": "vitest run --coverage",
    "build": "tsc && tsc-alias",
    "serve": "node dist/index.js",
    "lint": "eslint . --ext .ts,.tsx,.js,.mjs,.cjs",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.mjs,.cjs --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@koala-ts/framework": "^2.7.1",
    "pino": "^10.1.0",
    "remeda": "^2.33.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "globals": "^17.0.0",
    "pino-pretty": "^13.1.3",
    "pino-test": "^1.1.0",
    "prettier": "^3.7.4",
    "supertest": "^7.1.4",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.16"
  }
}
